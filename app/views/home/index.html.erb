<div class="row hero">
  <div class="theme-showcase">
    <div class="col-md-12">
      <div class="container" role="main">

        <div class="jumbotron">
          <h1>Researchers at Brown</h1>
        </div>

        <form id="search-homepage" action="<%= home_search_path() %>" name="search-home" role="search" method="post">
