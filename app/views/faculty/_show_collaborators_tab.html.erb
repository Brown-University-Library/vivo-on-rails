<div id="collabs" class="tab-pane fade in active" role="tabpanel">
  <h2 id="collabs" pgroup="tabs" class="hidden">Collaborators</h2>
  <div id="collabsGroup">

    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 id="relatedBy-Authorship" title="" class="panel-title">Collaborators</h3>
      </div>

      <div class="panel-body">
        <div class="property-list" role="list" id="relatedBy-Authorship-List" displaylimit="5">
          <table class="table table-hover">
            <tbody>
              <tr>
                <th>Name</th>
                <th>Title</th>
              </tr>
              <% @faculty.collaborators.each do |c| %>
                <tr class="tableRow" role="listitem">
                  <td>
                    <span class="listDateTime">
                      <a href="<%= c.uri %>"><%= c.name %></a>
                    </span>
                  </td>
                  <td>
                    <span class="listDateTime"><%= c.title %></span>
                  </td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
