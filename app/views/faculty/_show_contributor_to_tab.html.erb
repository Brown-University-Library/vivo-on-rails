<% if @faculty.contributor_to.count > 0 %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 id="relatedBy-Authorship" title="" class="panel-title">Contributor to </h3>
  </div>

  <div class="panel-body">
    <table class="table table-hover">
      <tbody>
        <% @faculty.contributor_to.each do |c| %>
          <tr class="publicationTableRow" role="listitem">
            <td class="citation-data">
              <span class="listDateTime"><%= c.authors %></span>
              <a href="<%= c.uri %>"><%= c.title %></a>
              <%= c.pub_info %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<% end %>
